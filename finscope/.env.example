# FinScope environment variables
# Rename to .env and fill in values. For Vite, use VITE_ prefix for frontend variables.

# General
NODE_ENV=development
PORT=4000
PYTHON_SERVICE_URL=http://localhost:8000

# Database
MONGO_URI=
MONGO_DB_NAME=finscope

# Supabase (Frontend expects VITE_ vars; backend may also use service role if needed)
SUPABASE_URL=
SUPABASE_KEY=
VITE_SUPABASE_URL=
VITE_SUPABASE_KEY=

# Google ADK / Gemini
ADK_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# External APIs
NEWS_API_KEY=
YAHOO_FINANCE_API_KEY=
ALPHAVANTAGE_API_KEY=
FRED_API_KEY=

# Frontend
VITE_API_BASE_URL=http://localhost:4000
