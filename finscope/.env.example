# Frontend origin for CORS (set to your deployed frontend URL)
# FRONTEND_ORIGIN=http://localhost:5173

# Plaid credentials
# PLAID_CLIENT_ID=
# PLAID_SECRET=
# PLAID_ENV=sandbox
# PLAID_REDIRECT_URI=

# Python service URL for Node
# PYTHON_SERVICE_URL=http://py-api:8000
##############################################
# FinScope Environment Example (.env.example)
##############################################

# Required (no demo mode)
ALPHAVANTAGE_API_KEY=
FRED_API_KEY=
ADK_API_KEY=
SUPABASE_URL=
SUPABASE_KEY=
MONGO_URI=
MONGO_DB_NAME=finscope

# Optional
# If not set, Node will call Python by docker service name (py-api)
# PYTHON_SERVICE_URL=http://py-api:8000

# News API (optional). If unset, falls back to Reuters RSS.
NEWS_API_KEY=

# Node API
PORT=4000

# Frontend build-time (Vite)
VITE_API_BASE_URL=http://localhost:4000
VITE_SUPABASE_URL=
VITE_SUPABASE_KEY=
VITE_AUTH_PROVIDER=email
VITE_PY_API_BASE_URL=http://localhost:8000